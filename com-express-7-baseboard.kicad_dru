(version 1)

(rule "100Ohm_diff_GbE_inner" 
	(constraint track_width (opt 0.124mm))
	(constraint clearance (opt 0.22mm))
	(condition "(A.NetClass == '100Ohm-diff_GbE')")
	(layer inner)
)

(rule "80Ohm_diff_USB_inner" 
	(constraint track_width (opt 0.159mm))
	(constraint clearance (opt 0.204mm))
	(condition "(A.NetClass == '90Ohm-diff_USB')")
	(layer inner)
)

